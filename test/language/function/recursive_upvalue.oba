fn makeClosure val {
  let outerVal = "outer"
  fn middle {
    let middleVal = "middle"
    fn inner {
      debug val
      debug outerVal
      debug middleVal 
    }
    inner
  }
  middle
}

let middle = makeClosure("arg")
let inner = middle()
inner()
