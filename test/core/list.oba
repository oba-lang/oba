import "list"
import "system"

{
  let xs = list::Empty()

  let size = 5
  let i = size
  while i > 0 {
    xs = list::append(i, xs)
    i = i - 1
  }

  fn odd x = x % 2 != 0

  let len = list::length(xs)
  let odds = list::format(list::where(odd, xs))
  let concatenated = list::format(list::concat(xs, xs))
  let values = list::format(xs)
  let sorted = list::format(list::sort(xs))

  system::println("Length is %(len)") // expect: Length is 5
  system::println("Values: %(values)") // expect: Values: 5,4,3,2,1
  system::println("Odds: %(odds)") // expect: Odds: 5,3,1
  system::println("Concatenated: %(concatenated)") // expect: Concatenated: 5,4,3,2,1,5,4,3,2,1
  system::println("Sorted: %(sorted)") // expect: Sorted: 1,2,3,4,5
}
