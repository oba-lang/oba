import "system"

// example: guides/language/values functions
fn hello = system::print("hello")
fn call f = f()
call(hello)
// end example
// expect: hello


// example: guides/language/values option
let none = None()
let some = Some("value")

system::print(none) // Prints: (Option::None)
system::print(some) // Prints: (Option::Some,value)
system::print(none == some) // Prints: false
// end example
// expect: (Option::None)
// expect: (Option::Some,value)
// expect: false

// example: guides/language/values importOption
import "option"

system::print(option::Some) // Prints: Option::Some
// end example
// expect: Option::Some

